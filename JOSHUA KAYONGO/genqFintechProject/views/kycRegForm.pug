doctype html
head
  meta(charset='utf-8')
  meta(name='viewport' content='width=device-width, initial-scale=1')
  link(rel='stylesheet' href='/CSS/Bootstrap/bootstrap-5.3.0-alpha1-dist/css/bootstrap.css')
  link(rel='stylesheet' href='/CSS/Bootstrap/bootstrap-5.3.0-alpha1-dist/custom.css')
  title kyc reg form
.container
  h1 GENQ KYC FORM
  h6 Bio Data
  .row.kyc
    .col
      input.form-control(type='text' name="firstName" placeholder='First name' aria-label='First name')
    .col
      input.form-control(type='text' name="lastName" placeholder='Last name' aria-label='Last name')
  .row.kyc
    .col
      input.form-control(type='text' name="dob" placeholder='Date Of Birth(DD/MM/YYY)' aria-label='Date of Birth')
    .col
      input.form-control(type='text' name="gender" placeholder='Gender' aria-label='Gender')
  h6  Location
  .row.kyc
    .col
      input.form-control(type='text' placeholder='Country' aria-label='Country')
    .col
      input.form-control(type='text' name="district" placeholder='State/district' aria-label='State/district')
  .row.kyc
    .col
      input.form-control(type='text' name="town" placeholder='Town' aria-label='Town')
    .col
      input.form-control(type='text' name="zipcode" placeholder='Zip Code' aria-label='Zip Code')
  h6  Contacts and address
  .row.kyc
    .col
      input.form-control(type='number' name="phone1" placeholder='phone number one' aria-label='phone number one')
    .col.kyc
      input.form-control(type='number' name="phone2" placeholder='phone number two' aria-label='phone number two')
  .row.kyc2
    .col
      input.form-control(type='email' name="email" placeholder='Email' aria-label='Email')
  .row
    .col
      button.btn.btn-primary(type='button') Primary
script(src='/CSS/Bootstrap/bootstrap-5.3.0-alpha1-dist/js/bootstrap.bundle.js')
